{
  "source": {
    "include": ["src"],

    "includePattern": ".+\\.js(doc|x)?$",

    "excludePattern": "(node_modules/|docs)"
  },

  "plugins": ["plugins/markdown"],

  "templates": {
    "cleverLinks": true,

    "monospaceLinks": true
  },

  "tags": {
    "allowUnknownTags": true,

    "dictionaries": ["jsdoc", "closure"]
  },

  "opts": {
    "recurse": true,
    "verbose": true,
    "template": "node_modules/clean-jsdoc-theme",
    "destination": "./docs/"
  }
}
